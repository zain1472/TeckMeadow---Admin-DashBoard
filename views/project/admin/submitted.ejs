            </div>
            <%}%> <%if(message && message.length>0){%>
            <div class="text-center alert alert-success" role="alert">
              <%=message%>
            </div>
            <%}%>

            <div class="row">
              <div class="col-md-12">
                <div class="card">
                  <div class="card-body">
                    <div class="table-responsive">
                      <table id="project-list" class="table table-lg">
                        <thead>
                          <tr>
                            <th>Project Title</th>
                            <th class="text-center">Payment</th>
                            <th class="text-center">Employee</th>
                            <th class="text-center">Status</th>
                            <th class="text-center">Started Date</th>
                            <th class="text-center">Due Date</th>
                            <th class="text-center">Completion Date</th>
                            <th class="text-center">Actions</th>
                          </tr>
                        </thead>
                        <tbody>
                          <%for(var i =projects.length -1;i>=0;i--){var project
                          = projects[i]%>
                          <tr>
                            <td>
                              <a href="/admin/project/<%=project._id%>"
                                ><%=project.title%></a
                              >
                            </td>
                            <td class="text-center">
                              <%=project.price%>&nbsp;Rs/-
                            </td>
                            <td class="text-center">
                              <div class="avatar-group">
                                <figure class="avatar avatar-sm">
                                  <a
                                    href="#"
                                    title="<%=project.employee.username%>"
                                    data-toggle="tooltip"
                                  >
                                    <img
                                      src="/uploads/<%=project.employee.image%>"
                                      class="rounded-circle"
                                      alt="avatar"
                                    />
                                  </a>
                                </figure>
                              </div>
                            </td>
                            <td class="text-center">
                              <%if(project.status == "ongoing"){%>
                              <span class="badge bg-info-bright text-info"
                                >In Progress</span
                              >
                              <%}else if(project.status == "completed"){%>
                              <span class="badge bg-info-bright text-success"
                                >Completed</span
                              >
                              <%}else if(project.status == "awaitingPayment"){%>
                              <span class="badge bg-info-bright text-success"
                                >Awaiting Payment</span
                              >
                              <%}else{%>
                              <span class="badge bg-danger-bright text-danger"
                                >Cancelled</span
                              >
                              <%}%>
                            </td>
                            <td class="text-info">
                              <%=project.dated.toDateString()%>
                            </td>
                            <td class="text-danger">
                              <%=project.dueDate.toDateString()%>
                            </td>
                            <%if(project.status == "completed"){%>
                            <td class="text-success">
                              <%=project.completionDate.toDateString()%> <%}%>
                            </td>

                            <td class="text-light">Not declared yet</td>
                            <td>
                              <%if(project.status =="awaitingPayment"){%>
                              <a
                                href="/admin/project/<%=project._id%>/completed"
                                class="text-success ml-2"
                                data-toggle="tooltip"
                                title="Mark Payment as Complete"
                              >
                                <i class=" fas fa-money-bill-wave"></i>
                              </a>
                              <%}%> <%if(project.status =="ongoing"){%>
                              <a
                                href="/admin/project/<%=project._id%>/awaitingPayment"
                                class="text-success ml-2"
                                data-toggle="tooltip"
                                title="Mark Project as Finished"
                              >
                                <i class=" fas fa-check"></i>
                              </a>
                              <a
                                href="/admin/project/<%=project._id%>/cancelled"
                                class="text-warning ml-2"
                                data-toggle="tooltip"
                                title="Cancel the Project"
                              >
                                <i class=" fas fa-window-close"></i>
                              </a>
                              <%}%>
                              <a
                                href="#"
                                class="text-secondary ml-2"
                                data-toggle="tooltip"
                                title="Edit"
                              >
                                <i class=" fas fa-pencil-alt"></i>
                              </a>
                              <a
                                href="/admin/project/<%=project._id%>/delete"
                                class="text-danger ml-2"
                                data-toggle="tooltip"
                                title="Delete"
                              >
                                <i class="fas fa-trash"></i>
                              </a>
                            </td>
                          </tr>
                          <%};%>
                        </tbody>
                      </table>
